{
  "expo": {
    "name": "Music Scanner",
    "description": "A simple barcode scanner app to help me organize my music collection",
    "slug": "music-scanner",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "githubUrl": "https://github.com/flammenmensch/music-scanner",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#333333"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false
    }
  }
}
